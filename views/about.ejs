<html lang="en">
<head>
    <%- include('layout/head') %>
</head>
<body>
    <nav>
        <%- include('layout/nav') %>
    </nav>
    <div>
        <table class="table table-stripped">
            <thead>
                <tr>
                  <th scope="col">No</th>
                  <th scope="col">Name</th>
                  <th scope="col">Email</th>
                  <th scope="col">ID</th>
                </tr>
              </thead>
              <tbody>
                <tr>
                  <% Datas.forEach((Member, i) => { i++%>
                    <th scope="row"><%= i++ %></th>
                    <th><%= Member.Name %></th>
                    <th><%= Member.email %></th>
                    <th><%= Member.ID %></th>
                  <% }) %>
                </tr>
              </tbody>
        </table>
    </div>
</body>
</html>